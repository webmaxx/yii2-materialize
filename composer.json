{
    "name": "altiore/yii2-materialize",
    "description": "The Materialize extension for the Yii framework",
    "keywords": ["yii2", "materialize"],
    "type": "yii2-extension",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/altiore/yii2-materialize/issues",
        "source": "https://github.com/altiore/yii2-materialize"
    },
    "authors": [
        {
            "name": "Razzwan",
            "email": "razvanlomov@gmail.com"
        },
        {
            "name": "webmaxx",
            "email": "webmaxx@webmaxx.name"
        }
    ],
    "require": {
        "yiisoft/yii2": "^2.0.0",
        "bower-asset/materialize": "^0.97.6"
    },
    "autoload": {
        "psr-4": {
            "altiore\\materialize\\": ""
        }
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
